{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/images/*.{png,jpg,gif,ico}", "/*.{css,js}", "/api/*" ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".json": "text/json",
    ".woff": "application/font-woff",
    ".woff2": "application/font-woff2"
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "X-Content-Type-Options": "nosniff"
  }
}
